@model IEnumerable<Applicant>
         
@{
    spacingModel modelValue = new spacingModel();
    modelValue.cellSpacing = new double[] { 10, 10 };
}


<div class="container">
    <div class="overview-container">
        <h4 class="overview-title"><strong>Summering av Sökande till YH-Utbildningar:</strong></h4>
        <div class="dashboard-container">
            <div class=" control-section">
                <!--  DashboardLayout element declaration -->
                <ejs-dashboardlayout id="defaultLayout" columns="6" cellSpacing="modelValue.cellSpacing">
                    <e-dashboardlayout-panels>
                        <!--Panel 1 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="0" col="0" content="<div>1</div>">

                        </e-dashboardlayout-panel>

                        <!--Panel 2 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="0" col="2" content="<div>2</div>">

                        </e-dashboardlayout-panel>

                        <!--Panel 3 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="0" col="4" content="<div>3</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 4 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="2" col="0" content="<div>4</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 5 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="2" col="2" content="<div>5</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 6 Kartvy-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="4" row="2" col="4" content="<div>6</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 7 Stapel/hockeyklubba-->
                        <e-dashboardlayout-panel sizeX="4" sizeY="2" row="4" col="0" content="<div>7</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 8 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="6" col="0" content="<div>8</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 9 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="6" col="2" content="<div>9</div>">
                        </e-dashboardlayout-panel>

                        <!--Panel 10 Donut-->
                        <e-dashboardlayout-panel sizeX="2" sizeY="2" row="6" col="4" content="<div>10</div>">
                        </e-dashboardlayout-panel>

                    </e-dashboardlayout-panels>
                </ejs-dashboardlayout>
                <!-- end of dashboardlayout element -->
            </div>
        </div>
    </div>
</div>
