@model IEnumerable<EducationDistribution>



<ejs-accumulationchart id="EducationDistrubtion" width="60%" height="60%">
    <e-accumulationchart-legendsettings visible="false" position="Right" alignment="Near" toggleVisibility="false" width="150px">
    </e-accumulationchart-legendsettings>
    <e-accumulationchart-tooltipsettings enable="true"></e-accumulationchart-tooltipsettings>
    <e-accumulation-series-collection>
        <e-accumulation-series dataSource="@(Model)" xName="Name" yName="NumberOfApplicants" name="Sökta Utbildningar" innerRadius="40%">
        </e-accumulation-series>
    </e-accumulation-series-collection>
</ejs-accumulationchart>
